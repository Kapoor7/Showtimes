<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="509.0" prefWidth="742.0" style="-fx-background-color: #373737;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MovieDetailsController">
   <children>
      <TableView fx:id="table" layoutX="14.0" layoutY="55.0" prefHeight="399.0" prefWidth="706.0" stylesheets="@mycss.css">
         <placeholder>
            <Label text="No Cinemas Currently Playing This Movie" />
         </placeholder>
         <columns>
            <TableColumn fx:id="colID" prefWidth="129.0" text="CinemaID" visible="false" />
            <TableColumn fx:id="colTitle" prefWidth="212.0" text="Cinema" />
            <TableColumn fx:id="coltime" minWidth="0.0" prefWidth="263.0" text="Time" />
            <TableColumn fx:id="colLaction" minWidth="0.0" prefWidth="229.0" text="Location" />
         </columns>
      </TableView>
      <JFXButton fx:id="btnHome" buttonType="RAISED" layoutX="25.0" layoutY="449.0" onAction="#btnHomeClicked" prefHeight="53.0" prefWidth="105.0" ripplerFill="#b90a0a" text="Home" textFill="#c0b283">
         <font>
            <Font name="Microsoft YaHei Light" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </JFXButton>
      <JFXButton fx:id="btnBack" buttonType="RAISED" layoutX="522.0" layoutY="449.0" onAction="#btnBackClicked" prefHeight="53.0" prefWidth="202.0" ripplerFill="#b90a0a" text="Go Back" textFill="#c0b283">
         <font>
            <Font name="Microsoft YaHei Light" size="18.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </JFXButton>
      <Label fx:id="lblPlaying" alignment="CENTER" layoutX="100.0" prefHeight="53.0" prefWidth="535.0" text="Cinemas Currently Playing: " textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Myanmar Text" size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
